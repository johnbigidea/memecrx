


{
  "manifest_version": 2,
  "name": "Test Extension",
  "description": "Example Chrome Extension",
  "version": "1.0",
  "author": "Jude Giordano",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "My custom extension"
  },
  "permissions": ["alarms", "notifications", "storage"],
  "content_scripts": [ {
  "matches": ["<all_urls>"],
  "js": ["j.js"]
} ]
}
